page {
  display: block;
  min-height: 100%;
  background-color: #fee6b1;
}

#container {
  width: 100%;
}

.icon_img {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.share-button {
  background: none;
  position: absolute;
  left: 0;
  width: 100%;
  height: 60rpx;
  margin-top: -10rpx;
}

.backImg {
  width: 100%;
  height: 860rpx;
  background-image: url("https://api.cfoeu.cn/uploads/20180205/5b93a4ff279dc3e23efafb72ec9eb28b.jpg");
  background-repeat: no-repeat;
  background-size: cover;
  position: relative;
}

/* 挑战规则框 */

.ChallengeRulesBox {
  width: 100%;
  height: 45rpx;
  position: absolute;
  top: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ChallengeSuccess {
  width: 330rpx;
  height: 45rpx;
  border: 1px solid fff;
  border-radius: 360rpx;
  background-color: #fff;
  color: #712613;
  font-size: 20rpx;
  position: absolute;
  left: 29%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ChallengeWord {
  width: 85%;
  height: 45rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
}

.ChallengePic {
  width: 45rpx;
  height: 45rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

/* 挑战规则按钮 */

/*挑战规则*/

.btn {
  width: 330rpx;
  height: 45rpx;
  border-radius: 360rpx;
  background-color: none;
  font-size: 20rpx;
  position: absolute;
  left: 29%;
  display: flex;
  justify-content: center;
  align-items: center;
}

/*mask*/

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  margin: 0 auto;
  border: 1px solid red;
}

/*content*/

.drawer_box {
  width: 650rpx;
  z-index: 1001;
  background: #fff5de;
  margin-top: 900rpx;
  border-radius: 3px;
  border: 1px solid #000;
}

.drawer_title {
  padding: 15px;
  font: 20px "microsoft yahei";
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.title_word {
  width: 300rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #da593a;
}

.closeBtn {
  width: 60rpx;
  height: 60rpx;
  background-color: #fdc500;
  border-radius: 360rpx;
  position: absolute;
  top: -15rpx;
  right: -15rpx;
  color: #fff;
  font-size: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.drawer_content {
  height: 600rpx;
  display: flex;
  justify-content: space-between;
  /* align-items: center; */
  flex-direction: column;
  overflow-y: scroll; /*超出父盒子高度可滚动*/
}

.ruleOne {
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin-top: 30rpx;
}

.ruleNo {
  width: 10%;
  display: flex;
  justify-content: center;
}

.ruleNo text {
  width: 50rpx;
  height: 50rpx;
  border-radius: 360rpx;
  background-color: #da593a;
  font-size: 35rpx;
  display: flex;
  justify-content: center;
  color: #fff;
  margin-top: 10rpx;
}

.ruleintroduce {
  width: 85%;
  justify-content: flex-start;
  padding: 10rpx;
  font-size: 26rpx;
  letter-spacing: 1rpx;
}

.btn_ok {
  height: 150rpx;
  padding: 10px;
  font: 20px "microsoft yahei";
  text-align: center;
  color: #000;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.challengeWord {
  width: 300rpx;
  height: 50rpx;
  font-size: 35rpx;
  color: #da593a;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ReceiveBtn {
  width: 300rpx;
  height: 80rpx;
  margin: 0 auto;
  background-color: #fdc500;
  border-radius: 360rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
}

/* 参赛次数样式 */

.frontNum {
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 25rpx;
  color: #742514;
  position: absolute;
  top: 360rpx;
}

.recordCount {
  width: 80%;
  height: 50rpx;
  margin: 0 auto;
  background-color: #db5937;
  display: flex;
  justify-content: center;
  align-items: center;
}

.startBox {
  width: 100%;
  height: 280rpx;
  position: absolute;
  top: 500rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.startBtn {
  width: 250rpx;
  height: 250rpx;
  background-repeat: no-repeat;
  background-size: contain;
}

.share {
  width: 100%;
  height: 80rpx;
  background-color: #cd533c;
  position: absolute;
  bottom: -15rpx;
}

.shareBox {
  width: 80%;
  height: 60rpx;
  border: 1px solid #fde3ae;
  border-radius: 360rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 10%;
  left: 10%;
}

.sharePic {
  width: 60rpx;
  height: 60rpx;
}

.shareText {
  width: 220rpx;
  display: inline-block;
}

.shareWord {
  font-size: 25rpx;
  color: #fde3ae;
}

/* 榜单选项卡 */

.swiper-tab {
  width: 95%;
  text-align: center;
  line-height: 80rpx;
  background-color: #fee7b1;
  display: flex;
  justify-content: space-around;
  align-content: center;
  margin: 30rpx 20rpx;
}

.swiper-tab-list {
  width: 33%;
  font-size: 25rpx;
  display: inline-block;
  color: #db5937;
  border: 1px solid #db5937;
}

.swiper-tab-list:nth-of-type(2) {
  border: none;
  border-top: 1px solid #db5937;
  border-bottom: 1px solid #db5937;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.swiper-box view {
  text-align: center;
}

.on {
  background-color: #db5937;
  color: #fee7b1;
  transition: all 0.3s linear;
}

